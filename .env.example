# Copy this file to .env and fill values.
# NEVER commit your real API keys.

# Pinecone
PINECONE_API_KEY=
PINECONE_INDEX=cuad-index
# Only needed for older pinecone SDK (pinecone.init)
PINECONE_ENV=

# Optional
EMBEDDING_MODEL=all-MiniLM-L6-v2
CONTRACT_ID=demo_contract

# Optional: Hugging Face access token (only needed for gated models / higher rate limits)
# Set either HF_TOKEN or HUGGINGFACEHUB_API_TOKEN
HF_TOKEN=
HUGGINGFACEHUB_API_TOKEN=

# Optional: seed demo users on backend startup (milestone3/backend)
# If unset, no demo users are created.
CLAUSEAI_DEMO_PASSWORD=

# Optional backend overrides
CLAUSEAI_BACKEND_DB_PATH=
CLAUSEAI_SESSION_TTL_HOURS=72
